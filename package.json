{
  "name": "webview-crypto",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/saulshanabrook/webview-crypto.git"
  },
  "description": "Helper code for crypto polyfil in a webview ",
  "scripts": {
    "prepublish": "webpack && ./build-webViewWorkerString.bash && tsc",
    "test:typings:install": "cd test; typings install",
    "test:lint": "tsc && cd test; tsc",
    "test": "cd test; zuul --no-coverage --browser-name ${BROWSER_NAME} --browser-version ${BROWSER_VERSION} -- testAll.ts",
    "test:local": "cd test; zuul --no-coverage --local -- testAll.ts"
  },
  "main": "./dist/index.js",
  "typings": "./src/index.ts",
  "files": [
    "dist",
    "src",
    "lib"
  ],
  "author": "Saul Shanabrook",
  "license": "MIT",
  "dependencies": {
    "lodash": "4.13.1",
    "serialize-error": "2.0.0"
  },
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-react-native": "1.9.0",
    "imports-loader": "0.6.5",
    "raw-loader": "0.5.1",
    "source-map-loader": "0.1.5",
    "tape": "4.6.0",
    "ts-loader": "0.8.2",
    "typescript": "1.8.10",
    "typings": "1.3.2",
    "webpack": "1.13.1",
    "webpack-fail-plugin": "1.0.4",
    "zuul": "3.10.2",
    "zuul-builder-webpack": "1.2.0",
    "zuul-localtunnel": "nolanlawson/zuul-localtunnel#https"
  }
}
